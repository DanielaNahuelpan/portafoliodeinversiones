<div id="<%= dom_id order %>">
  <p>
    <strong>Name:</strong>
    <%= order.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= order.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= order.phone %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= order.company %>
  </p>

  <p>
    <strong>Message:</strong>
    <%= order.message %>
  </p>

  <p>
    <strong>User:</strong>
    <%= order.user_id %>
  </p>

  <p>
    <strong>Property:</strong>
    <%= order.property_id %>
  </p>

</div>
